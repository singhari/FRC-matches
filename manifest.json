{
    "manifest_version": 3,
    "name": "FRC Matches (Raw HTML Ver.)",
    "description": "Lists a team's matches in a FRC event \"on deck\" / \"next up\" counter.",
    "version": "0.0.1",
    "action":{
        "default_popup": "extension.html"
    },
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "https://frc-api.firstinspires.org/v2.0/*"
    ] ,
    "background":{
        "service_worker": "scripts/service.js"
    },
    "options_ui": {
        "page": "settings.html",
        "open_in_tab": false
    }
}
